snippet % "block" wb
  {% ${1} %}

snippet bc "block comment" wb
  {#
    ${0:${VISUAL}}
  #}

snippet %% "block" b
  {% ${1:tag_name} %}
  ${0}
  {% end$1 %}

snippet { "curly" wb
  {{ ${1} }}

snippet if "if statement" wb
  {% if ${1} %}
    ${0}
  {% endif %}

snippet for "for loop" wb
  {% for ${1} %}
    ${0}
  {% endfor %}
